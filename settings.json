{
  "version": "0.1.0",
  "defaulttheme": "default",
  "website": {
    "port": 25000,
    "secret": "mySecret"
  },
  "pterodactyl": {
    "domain": "https://panel.example.com",
    "key": "ptla_000000000000000000000000000000000"
  },
  "blacklistedusers": ["userId", "anotherUserId"],
  "database": "database.sqlite",
  "api": {
    "client": {
       "api": {
         "enabled": true,
         "code": "myAPIKey"
       },
       "j4r": {
        "enabled": false,
        "servers": ["0000000000000000000","0000000000000000000"],
        "every": {
            "cpu": 5,
            "ram": 512,
            "disk": 2048,
            "servers": 1
        }
      },
       "bot": {
         "token": "myBotToken",
         "joinguild": {
           "_comment": "The Discord bot must be in these servers and have invite permissions.",
           "enabled": true,
           "guildid": ["0000000000000000000"],
           "forcejoin": true,
           "registeredrole": null
        }
      },
      "webhook": {
        "_comment": "URL of the webhook to send messages to.",
        "webhook_url": "myWebhookURL",
        "auditlogs": {
          "_comment": "This feature allows you to log actions, and get an overview of a users actions.",
          "enabled": true,
          "_comment2": "The types of notifications disabled. Values: ADMIN, LOGIN, SERVER, COUPONREDEEM",
          "disabled": []
        }
      },
      "passwordgenerator": {
        "signup": true,
        "length": 24
      },
      "allow": {
        "newusers": true,
        "regen": true,
        "server": {
          "create": true,
          "modify": true,
          "delete": true
        },
        "overresourcessuspend": false,
        "renewsuspendsystem": {
          "enabled": false,
          "time": 3600,
          "cost": 0
        }
      },
      "oauth2": {
        "_comment": "Go to https://discord.dev/ and create an application to set these up.",
        "id": "0000000000000000000",
        "secret": "secretHere",
        "link": "https://client.example.com",
        "callbackpath": "/callback",
        "prompt": true,

        "ip": {
          "trust x-forwarded-for": true,
          "block": [],

          "_comment": "'duplicate check' is an anti-alt check by IPs.",
          "duplicate check": true,

          "_comment2": "'cookie alt check' is an anti-alt check by cookies.",
          "cookie alt check": true
        }
      },
      "ratelimits": {
        "enabled": true,

        "trust x-forwarded-for": true,

        "requests": 30,
        "per second": 60
      },
      "packages": {
        "default": "default",
        "list": {
          "default": {
            "ram": 4096,
            "disk": 10240,
            "cpu": 100,
            "servers": 4
          }
        }
      },
      "locations": {
       "1": {
          "name": "UK-LON",
          "country": "United Kingdom",
          "city": "London",
          "iso": "GB",
          "package": null
        }
      },
      "eggs": {
        "paper": {
          "display": "Minecraft: Java Edition - Paper 1.20.4",
          "minimum": {
            "ram": 1024,
            "disk": 1024,
            "cpu": 50
          },
          "maximum": {
            "ram": null,
            "disk": null,
            "cpu": null
          },
          "info": {
            "egg": 5,
            "docker_image": "ghcr.io/pterodactyl/yolks:java_17",
            "startup": "java -Xms128M -XX:MaxRAMPercentage=95.0 -Dterminal.jline=false -Dterminal.ansi=true -jar {{SERVER_JARFILE}}",
            "environment": {
              "SERVER_JARFILE": "server.jar",
              "BUILD_NUMBER": "latest"
            },
            "feature_limits": {
              "databases": 1,
              "backups": 1
            }
          }
        },
        "bungeecord": {
          "display": "Minecraft: Java Edition -  BungeeCord",
          "minimum": {
            "ram": 512,
            "disk": 512,
            "cpu": 50
          },
          "maximum": {
            "ram": null,
            "disk": null,
            "cpu": null
          },
          "info": {
            "egg": 3,
            "docker_image": "ghcr.io/pterodactyl/yolks:java_17",
            "startup": "java -Xms128M -XX:MaxRAMPercentage=95.0 -jar {{SERVER_JARFILE}}",
            "environment": {
              "SERVER_JARFILE": "bungeecord.jar",
              "BUNGEE_VERSION": "latest"
            },
            "feature_limits": {
              "databases": 1,
              "backups": 1
            }
          }
        },
        "bedrock": {
          "display": "Minecraft: Bedrock Edition - Vanilla",
          "minimum": {
            "ram": 1024,
            "disk": 1024,
            "cpu": 50
          },
          "maximum": {
            "ram": null,
            "disk": null,
            "cpu": null
          },
          "info": {
            "egg": 21,
            "docker_image": "ghcr.io/parkervcp/yolks:debian",
            "startup": "./bedrock_server",
            "environment": {
              "BEDROCK_VERSION": "latest",
              "LD_LIBRARY_PATH": ".",
              "SERVERNAME": "Halex.gg",
              "GAMEMODE": "survival",
              "DIFFICULTY": "easy",
              "CHEATS": "true"
            },
            "feature_limits": {
              "databases": 1,
              "backups": 1
            }
          }
        },
        "pmmp": {
          "display": "Minecraft: Bedrock Edition - PocketmineMP",
          "minimum": {
            "ram": 1024,
            "disk": 1024,
            "cpu": 50
          },
          "maximum": {
            "ram": null,
            "disk": null,
            "cpu": null
          },
          "info": {
            "egg": 20,
            "docker_image": "ghcr.io/parkervcp/yolks:debian",
            "startup": "./bin/php7/bin/php ./PocketMine-MP.phar --no-wizard --disable-ansi",
            "environment": {
              "VERSION": "PM5"
            },
            "feature_limits": {
              "databases": 1,
              "backups": 1
            }
          }
        },
        "node": {
          "display": "Node.js - 20 LTS",
          "minimum": {
            "ram": 1024,
            "disk": 1024,
            "cpu": 50
          },
          "maximum": {
            "ram": null,
            "disk": null,
            "cpu": null
          },
          "info": {
            "egg": 18,
            "docker_image": "ghcr.io/parkervcp/yolks:nodejs_20",
            "startup": "if [[ -d .git ]] && [[ {{AUTO_UPDATE}} == \"1\" ]]; then git pull; fi; if [[ ! -z ${NODE_PACKAGES} ]]; then /usr/local/bin/npm install ${NODE_PACKAGES}; fi; if [[ ! -z ${UNNODE_PACKAGES} ]]; then /usr/local/bin/npm uninstall ${UNNODE_PACKAGES}; fi; if [ -f /home/container/package.json ]; then /usr/local/bin/npm install; fi; if [[ \"${MAIN_FILE}\" == \"*.js\" ]]; then /usr/local/bin/node \"/home/container/${MAIN_FILE}\" ${NODE_ARGS}; else /usr/local/bin/ts-node --esm \"/home/container/${MAIN_FILE}\" ${NODE_ARGS}; fi",
            "environment": {
              "USER_UPLOAD": "0",
              "AUTO_UPDATE": "0",
              "MAIN_FILE": "index.js"
            },
            "feature_limits": {
              "databases": 1,
              "backups": 1
            }
          }
        },
        "py": {
          "display": "Python - 3.12",
          "minimum": {
            "ram": 1024,
            "disk": 1024,
            "cpu": 50
          },
          "maximum": {
            "ram": null,
            "disk": null,
            "cpu": null
          },
          "info": {
            "egg": 19,
            "docker_image": "ghcr.io/parkervcp/yolks:python_3.12",
            "startup": "if [[ -d .git ]] && [[ \"{{AUTO_UPDATE}}\" == \"1\" ]]; then git pull; fi; if [[ ! -z \"{{PY_PACKAGES}}\" ]]; then pip install -U --prefix .local {{PY_PACKAGES}}; fi; if [[ -f /home/container/${REQUIREMENTS_FILE} ]]; then pip install -U --prefix .local -r ${REQUIREMENTS_FILE}; fi; /usr/local/bin/python /home/container/{{PY_FILE}}",
            "environment": {
              "USER_UPLOAD": "0",
              "AUTO_UPDATE": "0",
              "PY_FILE": "app.py",
              "REQUIREMENTS_FILE": "requirements.txt"
            },
            "feature_limits": {
              "databases": 1,
              "backups": 1
            }
          }
        }
      },
      "coins": {
        "enabled": true,
        "store": {
          "enabled": true,
          "ram": {
            "cost": 500,
            "per": 1024
          },
          "disk": {
            "cost": 200,
            "per": 5120
          },
          "cpu": {
            "cost": 500,
            "per": 100
          },
          "servers": {
            "cost": 1,
            "per": 1
          }
        }
      }
    },
    "arcio": {
      "enabled": true,
      "widgetid": "This has not been used since Heliactyl 11",
        "afk page": {
          "enabled": true,
          "path": "afkwspath",
          "every": 60,
          "coins": 1
        }
    }
  }
}
